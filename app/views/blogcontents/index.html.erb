<p id="notice"><%= notice %></p>

<h1>Blogcontents</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @blogcontents.each do |blogcontent| %>
      <tr>
        <td><%= blogcontent.content %></td>
        <td><%= blogcontent.user_id %></td>
        <td><%= link_to 'Show', blogcontent %></td>
        <td><%= link_to 'Edit', edit_blogcontent_path(blogcontent) %></td>
        <td><%= link_to 'Destroy', blogcontent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Blogcontent', new_blogcontent_path %>
